{
  "name": "spql",
  "version": "1.3.0",
  "description": "Query SharePoint's Web Services",
  "main": "lib/spql.js",
  "unpkg": "dist/spql.min.js",
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "cleanup": "rimraf lib dist",
    "prebuild": "npm run cleanup",
    "build": "npx rollup -c",
    "format": "pretty-quick",
    "lint": "eslint ./",
    "lint:fix": "eslint ./ --fix",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "jest --findRelatedTests"
    ],
    "./**/*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michaelaldave/spql.git"
  },
  "keywords": [
    "sharepoint",
    "soap",
    "query"
  ],
  "author": "Michael Aldave <michael.aldave@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/michaelaldave/spql/issues"
  },
  "homepage": "https://github.com/michaelaldave/spql#readme",
  "devDependencies": {
    "@babel/core": "7.1.6",
    "@babel/preset-env": "7.1.6",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.14.1",
    "eslint-config-prettier": "^4.0.0",
    "fast-async": "6.3.8",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.4",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "rimraf": "2.6.2",
    "rollup": "0.67.1",
    "rollup-plugin-babel": "4.0.3",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-terser": "3.0.0"
  }
}
